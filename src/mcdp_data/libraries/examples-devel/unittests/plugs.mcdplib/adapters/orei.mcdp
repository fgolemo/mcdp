# This device converts from TypeC to either TypeL or TypeA
mcdp {
    provides out [`AC_power]
    requires in  [`AC_power] 
    requires budget [USD]

    budget >= 7.31 USD

    take(required in,  1) >= `socket_type : TypeL

    take(provided out, 1) <= any-of({`socket_type : TypeA, `socket_type : TypeC})

    take(required in,  2) >=  take(provided out, 2) # voltages
    take(required in,  3) >=  take(provided out, 3) # power
}